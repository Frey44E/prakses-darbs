		<Properties>
			<ProtectedString name="Source"><![CDATA[local TextChatService = game:GetService("TextChatService")
local Players = game:GetService("Players")

local ChatTagText = "[Owner]"
local TagOwners = {
	["Frey36E"] = true,
}


local startColor = Color3.fromRGB(0, 0, 0)
local endColor   = Color3.fromRGB(255, 0, 0)


local function getGradientTag(text)
	local result = ""
	local len = #text
	for i = 1, len do
		local t = (i - 1) / math.max(len - 1, 1)
		local r = math.floor(startColor.R * 255 * (1 - t) + endColor.R * 255 * t)
		local g = math.floor(startColor.G * 255 * (1 - t) + endColor.G * 255 * t)
		local b = math.floor(startColor.B * 255 * (1 - t) + endColor.B * 255 * t)
		local char = text:sub(i, i)
		result = result .. string.format("<font color='rgb(%d,%d,%d)'>%s</font>", r, g, b, char)
	end
	return result
end


local function getFullTag()
	local gradientTag = getGradientTag(ChatTagText)
	local redSlash = string.format("<font color='rgb(%d,%d,%d)'></font>",
		math.floor(endColor.R * 255),
		math.floor(endColor.G * 255),
		math.floor(endColor.B * 255))
	return gradientTag .. " " .. redSlash
end

TextChatService.OnIncomingMessage = function(message)
	if message.TextSource then
		local senderName = Players:GetNameFromUserIdAsync(message.TextSource.UserId)
		if TagOwners[senderName] then
			local props = Instance.new("TextChatMessageProperties")
			props.PrefixText = getFullTag() .. " " .. (message.PrefixText or "")
			return props
		end
	end
	return nil
end]]></ProtectedString>
