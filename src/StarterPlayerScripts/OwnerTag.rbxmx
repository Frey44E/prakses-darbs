		<Properties>
			<ProtectedString name="Source"><![CDATA[-- Main Script (Local Script inside StarterPlayerScripts)

local TextChatService = game:GetService("TextChatService")

local Players = game:GetService("Players")



local r, g, b = 255, 176, 0

local ChatTagText = "[Owner]"

local TagOwners = {

    ["Frey36E"] = true,

	["Frey35E"] = true,

}



TextChatService.OnIncomingMessage = function(message: TextChatMessage)

    if message.TextSource then

        local senderName = Players:GetNameFromUserIdAsync(message.TextSource.UserId)

        if TagOwners[senderName] then

            local props = Instance.new("TextChatMessageProperties")

            props.PrefixText = string.format(

                "<font color='rgb(%d,%d,%d)'>%s</font> %s",

                r, g, b, ChatTagText, message.PrefixText or ""

            )

            return props

        end

    end



    return nil

end]]></ProtectedString>
